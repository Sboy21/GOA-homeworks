from turtle import *

num1 = int(input("please enter first number: "))
num2 = int(input("please enter second number: "))
num3 = int(input("please enter third number: "))

print(type(num1))
print(type(num2))
print(type(num3))

print(num1 / num2)
print(num1 // num2)
print(num1 % num2)
print(num1 / num3) 
print(num1 // num3)
print(num1 % num3)
print(num2 / num3)
print(num2 // num3)
print(num2 % num3)


